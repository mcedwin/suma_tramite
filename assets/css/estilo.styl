body
    background-color #eee
    font-size 13px
/*boostrap patch*/
.navbar
  margin-bottom 0
  border-bottom 1px solid #ccc
  h3
    margin-top 17px
  .navbar-header
    float none
  .navbar-brand
    display relative
    color #333 !important
    img.logo
      position absolute
      top 3px
      display block
      float left
      padding 5px 5px 0 0
    span
      margin-left 40px
h3
  font-size 16px
  margin 7px 5px 7px 0
/*label
  font-weight normal*/
.hand
  cursor pointer
.page-header
  margin 0
  border-bottom none
  .page-header-title
    border-bottom 1px solid #ccc
    padding-top 10px
    padding-bottom 10px
    margin-bottom 10px
    background-color: #ecf9ff

#page-wrapper.page-filter
  .page-header
    border-bottom 1px solid #ccc
  .page-content
    top 103px
#page-wrapper
    background-color #fff
.navbar
  .nav
    >li
      >a
        color #555
        white-space normal
        line-height 30px
    .dropdown
      float right
      .dropdown-messages
        width 310px
        margin-left -300px
        li>a
          white-space normal
.sidebar
  font-size 13px
  margin-top 50px
  .sidebar-nav
    background-color rgba(255, 255, 255, 1)
    background-image linear-gradient(to right, #eee 10px, #eee  10px)
    background-repeat repeat-x
    >ul>li>a.active,>ul>li.active
      background-image linear-gradient(to right, #999 10px, #ddd  10px)
      color #333
      text-decoration none
    ul
      padding 0
      margin 0
      list-style none
    a, a:hover, a:focus, a:active
      outline none
    ul li, ul a
      display block
    ul a
      padding 10px 10px 10px 20px
      color #333
      font-weight bold
      box-shadow 0px 1px 0px rgba(255, 255, 255, 0.05) inset
      i
        margin-right 5px
        font-size 14px
    ul a:hover, ul a:focus, ul a:active
      background-image linear-gradient(to right, #999 10px, #ddd  10px)
      color #000
      text-decoration none
    
    ul ul a.active
      background-image linear-gradient(to right, #206ca4 10px, #539cd1  10px)
      color #fff
      text-decoration none
    ul ul a
      padding 5px 30px 5px 50px
      font-weight normal
      background-image linear-gradient(to right, #ccc 10px, #f5f5f5  10px)
      border-bottom none
      line-height 20px
      span.more
        background-color #ddd
        color #fff
        padding 8px 5px 9px 5px
        float right
        position absolute
        top 0
        right 0
        width 25px
    ul ul a:hover, ul ul a:focus, ul ul a:active
      background-color rgba(255, 255, 255, 0.2)
      span.more
        background-color #aaa
      span.more:hover
        background-color #206ca4
    ul ul a.active
      span.more
        background-color #206ca4
    .sidebar-nav-item
      padding-left 5px
    .sidebar-nav-item-icon
      padding-right 5px
  .navbar-collapse
    padding 0
.flot-chart
  display block
  height 200px
.flot-chart-content
  width 100%
  height 100%
  
@media(min-width:768px)
  .sidebar
    margin-top 0
    z-index 1
    top 50px
    position absolute
    width 250px
    overflow auto
    bottom 0
  #page-wrapper
    position absolute
    top 50px
    left 250px
    right 0
    bottom 0
    border-left 1px solid #ccc
    overflow hidden
    .page-content
      overflow auto
      position absolute
      bottom 0
      left 0
      right 0
      top 53px
.btn-file
  position relative
  overflow hidden
  input[type=file]
    position absolute
    top 0
    right 0
    min-width 100%
    min-height 100%
    font-size 100px
    text-align right
    opacity 0
    outline none
    background white
    cursor inherit
    display block


